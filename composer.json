{
    "name": "duc_cnzj/es-builder",
    "description": "laravel builder for elasticsearch.",
    "license": "MIT",
    "authors": [
        {
            "name": "duc",
            "email": "1025434218@qq.com"
        }
    ],
    "keywords": [
        "duc",
        "laravel",
        "elasticsearch",
        "builder"
    ],
    "require": {
        "elasticsearch/elasticsearch": "^7.3",
        "illuminate/support": "5.8.*|^6.0",
        "illuminate/pagination": "5.8.*|^6.0",
        "illuminate/database": "5.8.*|^6.0",
        "illuminate/http": "5.8.*|^6.0"
    },
    "autoload": {
        "psr-4": {
            "DucCnzj\\EsBuilder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DucCnzj\\EsBuilder\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "^8.4",
        "orchestra/testbench": "^4.3"
    },
    "extra": {
        "laravel": {
            "providers": [
                "DucCnzj\\EsBuilder\\BuilderServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    }
}
